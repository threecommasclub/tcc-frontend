{
  "private": true,
  "name": "tcc-frontend",
  "version": "1.0.0",
  "repository": "git@github.com:threecommasclub/tcc-frontend.git",
  "scripts": {
    "start": "docker-compose -p tcc up",
    "build": "docker-compose -p tcc build",
    "stop": "docker-compose -p tcc stop",
