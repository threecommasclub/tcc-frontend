{
  "name": "tcc-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "./node_modules/.bin/eslint --ext js,jsx,ts,tsx ./",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "apollo-boost": "^0.4.4",
    "dotenv": "^8.1.0",
    "graphql": "^14.5.8",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.0.6",
    "react": "16.10.2",
    "react-apollo": "^3.1.2",
    "react-dom": "16.10.2",
    "react-map-gl": "^5.0.11",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/typescript": "1.8.1",
    "@graphql-codegen/typescript-operations": "1.8.1",
    "@graphql-codegen/typescript-react-apollo": "1.8.1",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@testing-library/react": "^9.3.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-map-gl": "^5.0.2",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "@zeit/next-css": "^1.0.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.1",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  }
}
